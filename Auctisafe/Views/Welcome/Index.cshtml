    // Function to update remaining time
    function updateRemainingTime() {
        @foreach (var product in Model)
        {
            if (product.auction.End_date.Date > DateTime.Now.Date && product.auction.Start_date<DateTime.Now)
            {
            <text>
                var remainingTime = new Date("@product.auction.End_date") - new Date();
        var remainingTimeFormatted = new Date(remainingTime).toISOString().substr(11, 8);
        var hours = parseInt(remainingTimeFormatted.substr(0, 2));
        var minutes = parseInt(remainingTimeFormatted.substr(3, 2));
        var seconds = parseInt(remainingTimeFormatted.substr(6, 2));
                var daysRemaining = Math.floor(remainingTime / (1000 * 60 * 60 * 24));
        var duration = daysRemaining + " Days : " + hours + " hours : " + minutes + " minutes : " + seconds + " Seconds"
        document.getElementById("remaining-time-@product.auction.Product_ID").textContent = duration;
            </text>}
            else if (product.auction.End_date.Date > DateTime.Now.Date && product.auction.Start_date > DateTime.Now)
            {
                 <text>
                var remainingTime = new Date("@product.auction.Start_date") - new Date();
        var remainingTimeFormatted = new Date(remainingTime).toISOString().substr(11, 8);
        var daysRemaining = Math.floor(remainingTime / (1000 * 60 * 60 * 24));
        var hours = parseInt(remainingTimeFormatted.substr(0, 2));
        var minutes = parseInt(remainingTimeFormatted.substr(3, 2));
        var seconds = parseInt(remainingTimeFormatted.substr(6, 2));
        var duration = daysRemaining + " Days : " + hours + " hours : " + minutes + " minutes : " + seconds +" Seconds"
            document.getElementById("remaining-time-@product.product.Product_ID").textContent = duration;
            </text>
            }
        }
    }

    // Update remaining time initially
    updateRemainingTime();

    // Update remaining time every second (1000 milliseconds)
    setInterval(updateRemainingTime, 1000);
</script>






